  <node pkg="robotont_driver" name="driver_node" type="driver_node" output="screen" />
  <node pkg="robotont_driver" name="serial_com_node" type="serial_com_node" />
  <node pkg="robotont_driver" name="odom_node" type="odom_node" output="screen"/>
</launch>
