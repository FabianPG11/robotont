<?xml version="1.0" ?>
<launch>
  <rosparam file="$(find robotont_driver)/config/wheel_params.yaml" command="load" />
  <node pkg="robotont_driver" name="driver_node" type="driver_node" />
  <node pkg="robotont_driver" name="odom_node" type="odom_node" />
  <node pkg="robotont_driver" name="serial_com_node" type="serial_com_node" />
  <node pkg="joy" name="joy_node" type="joy_node">
    <param name="deadzone" value="0.25" />
    <param name="autorepeat_rate" value="5" />
  </node>
  <node pkg="robotont_driver" name="robotont_teleop_joy" type="robotont_teleop_joy" />
</launch>
